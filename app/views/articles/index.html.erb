<h1>My articles</h1>
<ul>
  <% @articles.each do |article|%>
    <li><%= link_to article.title, article_path(article) %>, <%= link_to("delete", article_path(article), data: {turbo_method: :delete, turbo_confirm: "Are you sure?"}) %></li>
  <% end %>
</ul>
<p><%= link_to("Create new article", new_article_path) %></p>
